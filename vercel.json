{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "framework": "vite",
    "buildCommand": "npm run build --workspace=flowjob-frontend",
    "outputDirectory": "frontend-react/dist"
}